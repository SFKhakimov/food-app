{
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": "src",
        "strict": true,
        "target": "es6",
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "noEmit": true,
        "paths": {
            "common/*": ["common/*"]
        }
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
